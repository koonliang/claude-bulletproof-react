{
  "name": "bulletproof-react-backend",
  "version": "1.0.0",
  "description": "Backend API for Bulletproof React application",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:init": "tsx scripts/init-db.ts",
    "db:seed": "tsx scripts/seed-data.ts",
    "db:reset": "tsx scripts/reset-db.ts",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:setup:dev": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:ci": "cross-env NODE_ENV=test jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.4",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.11.1",
    "jest": "^30.0.4",
    "jest-mock-extended": "^4.0.0",
    "prisma": "^5.20.0",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
